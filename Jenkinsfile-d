pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is build phase'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test phase'
            }
            
        }
        stage('Deploy') {
            steps {
                echo 'This is Deploy phase'
                
            }
        }
        stage('Release') {
            steps{
                echo 'This is Relase phase'
            }
        }
        
    }
}
